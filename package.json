{
  "name": "cohabinator",
  "version": "0.0.0",
  "description": "Sync up with your roommates",
  "private": true,
  "main": "www/index.html",
  "scripts": {
    "dev": "webpack-dev-server --hot --progress --colors --content-base www/",
    "clean": "rm -rf ./public/*",
    "build": "npm run clean && NODE_ENV=production webpack --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/giant-shoulders/cohabinator.git"
  },
  "keywords": [
    "cohabinator",
    "roommates",
    "react",
    "redux",
    "postcss",
    "cssnext"
  ],
  "author": "Alex Howard (alex.howard@giantshoulders.net)",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "debounce": "^1.0.0",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "isomorphic-fetch": "^2.2.1",
    "milligram-cssnext": "^1.0.1",
    "postcss": "^5.1.1",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.4.1",
    "postcss-url": "^5.1.2",
    "react": "^15.2.1",
    "react-addons-update": "^15.3.0",
    "react-dom": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "extends": "airbnb",
    "rules": {
      "react/prefer-stateless-function": 0,
      "no-underscore-dangle": 0,
      "max-len": 0
    },
    "globals": {
      "__DEV__": true
    }
  }
}
